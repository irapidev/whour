{
  "name": "whour",
  "version": "0.0.0",
  "description": "WhoUR is an IP to location service.",
  "main": "dist/server/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "git:commit": "git-cz",
    "npm:version:major": "npm version major --no-git-tag-version",
    "npm:version:minor": "npm version minor --no-git-tag-version",
    "npm:version:patch": "npm version patch --no-git-tag-version",
    "test:types": "tsc --noEmit",
    "test:types:watch": "npm run test:types -- --watch",
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidevelop/whour.git"
  },
  "keywords": [],
  "author": "Iman Vafadarnezhad",
  "license": "MIT",
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/rapidevelop/whour/issues"
  },
  "homepage": "https://github.com/rapidevelop/whour#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.0.2",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "typescript": "^3.4.5"
  }
}
